import java.util.Scanner;

public class Ejercicio11 {

	public static void main(String[] args) {
		int num;
		int cont=0;
		int k=1;
		Scanner teclado = new Scanner(System.in);
		System.out.println("Inserte un número:");
		num = teclado.nextInt();
		
		for(num % 3 == 0){
			for(k = 1; k <= num; k = k + 1);
			System.out.println(k);
			}
			cont = cont + 1;
		}
		System.out.println(cont);
	}

}
